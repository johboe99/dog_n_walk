<div class="container">
  <% @dogs.each do |dog| %>
    <div class="card bg-white my-3 px-3 py-3">
      <h2><%= dog.name%></h2>
      <p><%= dog.breed %>, <%= dog.age %> years old</p>
      <p><%= dog.description %></p>
      <%= link_to 'Find out more!', dog, class: 'btn btn-primary' %>
    </div>
  <% end %>
</div>

